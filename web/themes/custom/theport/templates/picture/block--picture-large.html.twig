{{ attach_library('theport/picture') }}
{%
  set classes = [
    'block',
    'block-' ~ configuration.provider|clean_class,
    'block-' ~ plugin_id|clean_class,
  ]
%}
<div{{attributes.addClass(classes)}}>
  <div class="picture-large">
    {% block content %}
      <div{{content_attributes.addClass('content')}}>
        {{ content }}
      </div>
    {% endblock %}
  </div>
</div>
