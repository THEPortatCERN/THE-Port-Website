{
  "files.exclude": {
    "**/composer.lock": true,
    "**/package-lock.json": true,
    "**/.gitattributes": true,
    "**/.editorconfig": true,
    "**/LICENSE.txt": true,
    "data/": true,
    "vendor/": false,
    "web/.csslintrc": true,
    "web/.eslintignore": true,
    "web/.eslintrc.json": true,
    "web/.ht.router.php": true,
    "web/.htaccess": true,
    "web/autoload.php": true,
    "web/example.gitignore": true,
    "web/index.php": true,
    "web/INSTALL.txt": true,
    "web/README.txt": true,
    "web/robots.txt": true,
    "web/update.php": true,
    "web/web.config": true,
    "web/core": false,
    "web/libraries": false,
    "web/modules/": false,
    "web/modules/README.txt": true,
    "web/modules/contrib": false,
    "web/profiles": true,
    "web/sites/README.txt": true,
    "web/sites/example.*": true,
    "web/sites/default/files/": true,
    "web/sites/default/settings.php": true,
    "web/sites/default/default.settings.php": true,
    "web/sites/default/default.services.yml": true,
    "web/themes/README.txt": true,
    "web/themes/contrib/bootstrap5/_SUBTHEME": true
  },
  "php.suggest.basic": false,
  "intelephense.environment.documentRoot": "web/core/index.php",
  "intelephense.diagnostics.undefinedTypes": false,
  "intelephense.format.braces": "k&r"
}
